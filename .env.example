# Atlas MemberClicks API Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Atlas MemberClicks client secret
ATLAS_CLIENT_SECRET=your_client_secret_here

# Required: OpenAI API key for CopilotKit functionality
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override default values if needed
ATLAS_CLIENT_ID=CarlsbadChamber
ATLAS_TENANT=carlsbad
ATLAS_AUTH_URL=https://www.weblinkauth.com/connect/token
ATLAS_API_BASE_URL=https://api-v1.weblinkconnect.com/api-v1
